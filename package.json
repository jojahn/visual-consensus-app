{
    "name": "visual-consensus",
    "version": "1.0.1",
    "entry": "index.mjs",
    "module": "esnext",
    "private": true,
    "scripts": {
        "start": "vite",
        "build": "vite build",
        "preview": "vite preview",
        "legacy-start": "echo 'unsupported: webpack-dev-server --mode development'",
        "legacy-build": "echo 'unsupported: webpack --mode production'",
        "test": "jest",
        "vitest": "vitest",
        "test:watch": "jest --watch",
        "lint": "eslint . --ext .ts,.js,.mjs",
        "lint:fix": "eslint . --fix --ext .ts,.js,.mjs"
    },
    "eslintConfig": {
        "extends": [
            "standard"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@babel/core": "^7.18.9",
        "@babel/preset-env": "^7.18.9",
        "@grpc/grpc-js": "^1.7.1",
        "@grpc/proto-loader": "^0.7.3",
        "@rollup/plugin-json": "^4.1.0",
        "@types/jest": "^28.1.6",
        "@typescript-eslint/eslint-plugin": "^5.33.0",
        "@typescript-eslint/parser": "^5.33.0",
        "babel-jest": "^28.1.3",
        "babel-preset-vite": "^1.0.4",
        "eslint": "^8.21.0",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-svelte3": "^4.0.0",
        "google-protobuf": "^3.21.0",
        "jest": "^28.1.3",
        "jest-environment-jsdom": "^28.1.3",
        "svelte-jester": "^2.3.2",
        "svelte-loader": "^3.1.3",
        "ts-jest": "^28.0.7",
        "typescript-eslint": "0.0.1-alpha.0",
        "vitest": "^0.24.3"
    },
    "dependencies": {
        "@sveltejs/vite-plugin-svelte": "^1.0.0-next.42",
        "@vitejs/plugin-legacy": "^1.8.1",
        "jsonpack": "^1.1.5",
        "messagepack": "^1.1.12",
        "svelte": "^3.48.0",
        "typescript": "^4.7.4",
        "vite": "^2.9.5"
    },
    "jest": {
        "moduleFileExtensions": [
            "mjs",
            "js",
            "jsx",
            "ts",
            "tsx",
            "json",
            "svelte"
        ],
        "testMatch": [
            "**/?(*.)test.?js",
            "**/?(*.)test.ts"
        ],
        "testPathIgnorePatterns": [
            "/node_modules/"
        ],
        "transform": {
            "^.+\\.m?js$": "babel-jest",
            "^.+\\.ts?$": "ts-jest",
            "^.+\\.svelte$": "svelte-jester"
        },
        "testEnvironment": "jsdom"
    }
}
